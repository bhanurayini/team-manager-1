<p id="notice"><%= notice %></p>

<h1>Team Has Coaches</h1>

<table>
  <thead>
    <tr>
      <th>Coach type</th>
      <th>Person</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @team_has_coaches.each do |team_has_coach| %>
      <tr>
        <td><%= team_has_coach.coach_type %></td>
        <td><%= team_has_coach.person %></td>
        <td><%= team_has_coach.team %></td>
        <td><%= link_to 'Show', team_has_coach %></td>
        <td><%= link_to 'Edit', edit_team_has_coach_path(team_has_coach) %></td>
        <td><%= link_to 'Destroy', team_has_coach, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team Has Coach', new_team_has_coach_path %>
